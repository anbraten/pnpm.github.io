#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/runner/work/pnpm.github.io/pnpm.github.io/benchmarks/node_modules/.pnpm/esprima@4.0.1/node_modules/esprima/bin/node_modules:/home/runner/work/pnpm.github.io/pnpm.github.io/benchmarks/node_modules/.pnpm/esprima@4.0.1/node_modules/esprima/node_modules:/home/runner/work/pnpm.github.io/pnpm.github.io/benchmarks/node_modules/.pnpm/esprima@4.0.1/node_modules:/home/runner/work/pnpm.github.io/pnpm.github.io/benchmarks/node_modules/.pnpm/node_modules:/home/runner/work/pnpm.github.io/pnpm.github.io/benchmarks/node_modules:/home/runner/work/pnpm.github.io/pnpm.github.io/node_modules:/home/runner/work/pnpm.github.io/node_modules:/home/runner/work/node_modules:/home/runner/node_modules:/home/node_modules:/node_modules:/home/runner/work/pnpm.github.io/pnpm.github.io/benchmarks/node_modules/.pnpm/js-yaml@3.14.1/node_modules/js-yaml/node_modules:/home/runner/work/pnpm.github.io/pnpm.github.io/benchmarks/node_modules/.pnpm/js-yaml@3.14.1/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../esprima@4.0.1/node_modules/esprima/bin/esparse.js" "$@"
else
  exec node  "$basedir/../../../../../esprima@4.0.1/node_modules/esprima/bin/esparse.js" "$@"
fi
