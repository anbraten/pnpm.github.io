#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/runner/work/pnpm.github.io/pnpm.github.io/benchmarks/node_modules/.pnpm/get-folder-size@1.0.1/node_modules/get-folder-size/bin/node_modules:/home/runner/work/pnpm.github.io/pnpm.github.io/benchmarks/node_modules/.pnpm/get-folder-size@1.0.1/node_modules/get-folder-size/node_modules:/home/runner/work/pnpm.github.io/pnpm.github.io/benchmarks/node_modules/.pnpm/get-folder-size@1.0.1/node_modules:/home/runner/work/pnpm.github.io/pnpm.github.io/benchmarks/node_modules/.pnpm/node_modules:/home/runner/work/pnpm.github.io/pnpm.github.io/benchmarks/node_modules:/home/runner/work/pnpm.github.io/pnpm.github.io/node_modules:/home/runner/work/pnpm.github.io/node_modules:/home/runner/work/node_modules:/home/runner/node_modules:/home/node_modules:/node_modules:/home/runner/work/pnpm.github.io/pnpm.github.io/benchmarks/node_modules/get-folder-size/bin/node_modules:/home/runner/work/pnpm.github.io/pnpm.github.io/benchmarks/node_modules/get-folder-size/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../get-folder-size/bin/get-folder-size" "$@"
else
  exec node  "$basedir/../get-folder-size/bin/get-folder-size" "$@"
fi
