#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/runner/work/pnpm.github.io/pnpm.github.io/benchmarks/node_modules/.pnpm/npm@7.7.5/node_modules/npm/bin/node_modules:/home/runner/work/pnpm.github.io/pnpm.github.io/benchmarks/node_modules/.pnpm/npm@7.7.5/node_modules/npm/node_modules:/home/runner/work/pnpm.github.io/pnpm.github.io/benchmarks/node_modules/.pnpm/npm@7.7.5/node_modules:/home/runner/work/pnpm.github.io/pnpm.github.io/benchmarks/node_modules/.pnpm/node_modules:/home/runner/work/pnpm.github.io/pnpm.github.io/benchmarks/node_modules:/home/runner/work/pnpm.github.io/pnpm.github.io/node_modules:/home/runner/work/pnpm.github.io/node_modules:/home/runner/work/node_modules:/home/runner/node_modules:/home/node_modules:/node_modules:/home/runner/work/pnpm.github.io/pnpm.github.io/benchmarks/node_modules/npm/bin/node_modules:/home/runner/work/pnpm.github.io/pnpm.github.io/benchmarks/node_modules/npm/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../npm/bin/npx-cli.js" "$@"
else
  exec node  "$basedir/../npm/bin/npx-cli.js" "$@"
fi
